<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="dkplus.class_diagram.page_processor" class="Dkplus\LivingDocs\ClassDiagramExtension\ClassDiagramPageProcessor">
            <argument type="service" id="dkplus.source_code.dependencies" />
            <argument type="service" id="dkplus.source_code.packages" />
            <tag name="dkplus.pages.processor" page_type="class_diagram" />
            <tag name="dkplus.source_code.annotation_subscriber" />
        </service>
    </services>
</container>
