{
    "name": "dkplus/living-docs",
    "description": "Living Docs compiler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Bley",
            "email": "oskar@programming-php.net"
        }
    ],
    "bin": [
        "bin/dkplus"
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dkplus\\LivingDocumentation\\": "src"
        }
    },
    "require": {
        "beberlei/assert": "^2.7",
        "behat/gherkin": "^4.4",
        "doctrine/annotations": "^1.4",
        "michelf/php-markdown": "^1.7",
        "nikic/php-parser": "^3.0",
        "symfony/config": "^3.2",
        "symfony/console": "^3.2",
        "symfony/dependency-injection": "^3.2",
        "symfony/finder": "^3.2",
        "symfony/yaml": "^3.2",
        "twig/twig": "^2.0",
        "zendframework/zend-code": "^3.1",
        "league/commonmark": "^0.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpspec/phpspec": "^3.4"
    }
}
